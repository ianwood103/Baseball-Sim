<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="ap" prefHeight="600.0" prefWidth="600.0" stylesheets="@Main.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Pitch Location Picker">
               <font>
                  <Font size="28.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="40.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER" prefWidth="200.0">
               <children>
                  <Region />
                  <GridPane prefHeight="400.0" prefWidth="400.0" styleClass="pane">
                     <children>
                        <Pane fx:id="pane1" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                        <Pane fx:id="pane2" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.columnIndex="1" />
                        <Pane fx:id="pane3" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.columnIndex="2" />
                        <Pane fx:id="pane4" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.rowIndex="1" />
                        <Pane fx:id="pane5" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Pane fx:id="pane6" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                        <Pane fx:id="pane7" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.rowIndex="2" />
                        <Pane fx:id="pane8" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Pane fx:id="pane9" prefHeight="200.0" prefWidth="200.0" styleClass="pane" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                     </children>
                     <columnConstraints>
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                  </GridPane>
                  <Region />
               </children>
            </HBox>
            <Button fx:id="button" mnemonicParsing="false" onAction="#pickLocation" prefHeight="50.0" prefWidth="300.0" style="-fx-cursor: pointer;" text="Pick Location">
               <VBox.margin>
                  <Insets bottom="-0.0" top="40.0" />
               </VBox.margin>
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
