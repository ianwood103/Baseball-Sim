<AnchorPane fx:id="ap" prefHeight="700.0" prefWidth="1200.0" stylesheets="@Stats.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StatsController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <GridPane hgap="20.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Batting Average:" />
                  <Label text="On-Base Percentage:" GridPane.rowIndex="1" />
                  <Label text="Slugging Percentage:" GridPane.rowIndex="2" />
                  <Label text="OPS:" GridPane.rowIndex="3" />
                  <Label text="Home Runs:" GridPane.rowIndex="4" />
                  <Label text="Plate Appearances:" GridPane.rowIndex="5" />
                  <Label fx:id="averageLabel" text="null" GridPane.columnIndex="1" />
                  <Label fx:id="obpLabel" text="null" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="slgLabel" text="null" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="opsLabel" text="null" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="homerLabel" text="null" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="paLabel" text="null" GridPane.columnIndex="1" GridPane.rowIndex="5" />
               </children>
               <BorderPane.margin>
                  <Insets right="60.0" />
               </BorderPane.margin>
            </GridPane>
         </center>
         <top>
            <Label text="Statistics:" BorderPane.alignment="CENTER">
               <font>
                  <Font size="34.0" />
               </font>
               <BorderPane.margin>
                  <Insets bottom="20.0" top="30.0" />
               </BorderPane.margin>
            </Label>
         </top>
         <bottom>
            <Button mnemonicParsing="false" onAction="#restart" prefHeight="40.0" prefWidth="160.0" text="Continue" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="30.0" top="20.0" />
               </BorderPane.margin>
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
